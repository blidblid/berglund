{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rx": {
      "projectType": "library",
      "root": "projects/rx",
      "sourceRoot": "projects/rx/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rx/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rx/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rx/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rx/src/test.ts",
            "tsConfig": "projects/rx/tsconfig.spec.json",
            "karmaConfig": "projects/rx/karma.conf.js"
          }
        }
      }
    },
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "viewEncapsulation": "None",
          "prefix": "bop",
          "export": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/app",
      "sourceRoot": "projects/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "projects/app/src/index.html",
            "main": "projects/app/src/main.ts",
            "polyfills": "projects/app/src/polyfills.ts",
            "tsConfig": "projects/app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app/src/favicon.ico",
              "projects/app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/app/src/environments/environment.ts",
                  "with": "projects/app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "tsConfig": "projects/app/tsconfig.app.prod.json"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "development": {
              "browserTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        }
      }
    },
    "components": {
      "projectType": "library",
      "root": "projects/components",
      "sourceRoot": "projects/components/src",
      "prefix": "lib",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "viewEncapsulation": "None",
          "prefix": "bop",
          "export": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/components/src/test.ts",
            "tsConfig": "projects/components/tsconfig.spec.json",
            "karmaConfig": "projects/components/karma.conf.js"
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "projects/material",
      "sourceRoot": "projects/material/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/material/src/test.ts",
            "tsConfig": "projects/material/tsconfig.spec.json",
            "karmaConfig": "projects/material/karma.conf.js"
          }
        }
      }
    },
    "firebase": {
      "projectType": "library",
      "root": "projects/firebase",
      "sourceRoot": "projects/firebase/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/firebase/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/firebase/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/firebase/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/firebase/src/test.ts",
            "tsConfig": "projects/firebase/tsconfig.spec.json",
            "karmaConfig": "projects/firebase/karma.conf.js"
          }
        }
      }
    },
    "mixins": {
      "projectType": "library",
      "root": "projects/mixins",
      "sourceRoot": "projects/mixins/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mixins/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mixins/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mixins/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mixins/src/test.ts",
            "tsConfig": "projects/mixins/tsconfig.spec.json",
            "karmaConfig": "projects/mixins/karma.conf.js"
          }
        }
      }
    }
  }
}
